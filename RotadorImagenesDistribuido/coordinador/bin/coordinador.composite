<?xml version="1.0" encoding="UTF-8"?>
<composite xmlns="http://www.osoa.org/xmlns/sca/1.0" xmlns:frascati="http://frascati.ow2.org/xmlns/sca/1.1"
           targetNamespace="http://frascati.ow2.org/comunicacion"
           name="coordinador">
  <component name="c">
    <implementation.java class="coordinador.CoordinadorImpl"/>
    <service name="r">
      <interface.java interface="servicios.Coordinador" />
    </service>
    <reference name="rotadorImagenes">
    	 <interface.java interface="servicios.RotadorImagenes" />
    </reference>
  </component>
  <service name="r" promote="c/r">
    <interface.java interface="servicios.Coordinador"/>
  </service>
  <reference name="rotadorImagenes" promote="c/rotadorImagenes">
  	<frascati:binding.rmi host="localhost" serviceName="rmirotarimagen" port="1100"/>
	 <interface.java interface="servicios.RotadorImagenes" />
  </reference>
 
</composite>

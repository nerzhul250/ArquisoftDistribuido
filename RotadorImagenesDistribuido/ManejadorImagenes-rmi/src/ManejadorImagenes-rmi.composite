<composite xmlns="http://www.osoa.org/xmlns/sca/1.0"
  xmlns:frascati="http://frascati.ow2.org/xmlns/sca/1.1"
  targetNamespace="http://frascati.ow2.org/arquisoft-distribuido-rmi"
  name="ManejadorImagenesRMI">

  <reference name="coordinator" promote="ManejadorImagenes/coordinator">
      <interface.java interface="cargador.RotadorImagenes" />
      <frascati:binding.rmi host="localhost" serviceName="rmirotadorImagenesCoordinator" port="1099"/>
  </reference>

  <component name="ManejadorImagenes">

    <reference name="coordinator">
      <interface.java interface="cargador.RotadorImagenes" />
    </reference>

    <implementation.java class="cargador.Cargador" />

  </component>

</composite>